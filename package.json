{
  "name": "plain-app",
  "version": "0.1.1",
  "title": "plain-app",
  "description": "A app components Library based on Vue.js",
  "keywords": [
    "vue",
    "vue.js",
    "component",
    "components",
    "ui",
    "framework"
  ],
  "main": "dist/pl-app.umd.min.js",
  "files": [
    "dist"
  ],
  "author": "martsforever",
  "license": "MIT",
  "scripts": {
    "start": "npm run serve",
    "serve": "vue-cli-service serve",
    "build:page": "vue-cli-service build",
    "build": "vue-cli-service build --target lib --name pl-app src/index.js --dest dist",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "better-scroll": "^1.13.2",
    "fecha": "^3.0.2",
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.2.0",
    "@vue/cli-plugin-eslint": "^3.2.0",
    "@vue/cli-service": "^3.2.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "file-loader": "^3.0.1",
    "highlight.js": "^9.13.1",
    "node-sass": "^4.10.0",
    "sass-loader": "^7.1.0",
    "text-loader": "0.0.1",
    "vue-template-compiler": "^2.6.6"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
